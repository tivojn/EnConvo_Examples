{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "js/**", "use": "@vercel/static" },
    { "src": "css/**", "use": "@vercel/static" },
    { "src": "icon.png", "use": "@vercel/static" },
    { "src": "Latin-Vocab-Shadcn/server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/latin-vocab/(.*)", "dest": "/Latin-Vocab-Shadcn/server.js" },
    { "src": "/api/(.*)", "dest": "/Latin-Vocab-Shadcn/server.js" },
    { "src": "/$", "dest": "/index.html" },
    { "src": "/(.+)\\.(js|css|png|jpg|svg|ico)$", "dest": "/$1.$2" }
  ]
}